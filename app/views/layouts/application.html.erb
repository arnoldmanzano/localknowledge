<!DOCTYPE html>
<html ng-app="LocalKnowledgeApp">
<head>
  <title>LocalKnowledge</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script src="https://maps.googleapis.com/maps/api/js?libraries=places,geometry" async defer></script>
</head>
<body>
  <header>
    <nav>
    <%= render template: "layouts/_nav_bar.html.erb" %>
    <nav>
  <header>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>

</body>
</html>
